class Solution {
    public int areaOfMaxDiagonal(int[][] dimensions) {
        int n =dimensions.length;
        int max=0;
        int magd=0;
        for(int i=0;i<n;i++){
            int l=dimensions[i][0];
            int w=dimensions[i][1];
            int curr=l*l+w*w;
            if(curr>magd || (curr==magd && l*w>max)){
                magd=curr;
                max=l*w;
            }
        }
        return max;
    }
}
